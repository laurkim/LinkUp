<%= form_for @user, url: login_path, method: :post do |f| %>
  <%= f.label :username %><br>
  <%= f.text_field :username %><br>

  <%= f.label :password %><br>
  <%= f.password_field :password %><br>

  <%= f.submit "Log In" %>
  <br>
  Or <%= link_to(controller: 'users', action: 'new') { 'Sign Up' } %>
<% end %>
